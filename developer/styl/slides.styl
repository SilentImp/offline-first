article.files.files.files
  background url('../images/files.jpg') 50% 50% no-repeat
  background-size cover
  h1
    left 0
    right 60px
    bottom 100px
    margin auto
    position absolute
    padding 40px 0
    text-align center
    box-sizing border-box
    width 300px
    height 300px
    line-height 220px
    font-size 48px
    white-space nowrap
    &:after
      visibility hidden
      opacity 0
      margin 0 0 0 -20px
      transform-origin 20px -150px
      transform rotate(-40deg)
      top 295px
      border-width: 60px 20px 0;

