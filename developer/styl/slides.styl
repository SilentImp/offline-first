article.bgg.bgg.bgg
  background url('../images/bgg.jpg') 100% 100% no-repeat
  background-size cover
  h1
    left 40px
    top 60px
    margin auto
    position absolute
    padding 40px 0
    text-align center
    box-sizing border-box
    width 400px
    height 400px
    line-height 260px
    font-size 160px
    white-space nowrap
    background-color #ec174f
    color #fff
    .wrapper
      line-height 1.43
    &:after
      margin 0 0 0 -20px
      transform-origin 20px -200px
      transform rotate(-68deg)
      top 395px
      border-top-color #ec174f
      border-width: 60px 20px 0;

