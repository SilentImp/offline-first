article.next.next.next
  h1
    position absolute
    top 0
    bottom 300px
    overflow hidden
    left 0
    right 0
    width 200px
    height 200px
    line-height 200px
    border-radius 100%
    margin auto
    display block
    background-color #fff
    span
      position absolute
      top 0
      bottom 0
      right 0
      width 100px
      display block
      color #fff
      background-color #ec174f
  .half
    h2
      font-size 36px
    &:first-child
      background-color #ec174f
      color #fff

article.process.process.process
  .half
    h2
      font-size 20px
      .sub-wrapper
        display inline-block
        vertical-align middle
        line-height 1.2
        text-align left
    .wrapper-inner
      display inline-block
      vertical-align middle
      height 400px
      line-height 1
      h1
        font-size 30px
        white-space nowrap
      ul
        font-size 20px
        line-height 24px
    &:last-child
      background-color #ec174f
      color #fff

.half
  h3
    font-size 48px
    line-height 56px
    display block
    text-align center
    padding-left 40px
    span
      text-align left

